"*":
  core:
    closeDeletedFileTabs: true
    disabledPackages: [
      "exception-reporting"
    ]
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
  editor:
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  "go-plus":
    lint:
      args: [
        "--vendor"
      ]
    test: {}
  "linter-shellcheck":
    enableNotice: true
  "linter-ui-default": {}
  welcome:
    showOnStartup: false
".go.source":
  editor:
    tabType: "hard"
